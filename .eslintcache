Recetas();\r\n        }\r\n        \r\n\r\n    },[busqueda]);\r\n\r\n    return (\r\n        <RecetasContext.Provider\r\n            value={{\r\n                recetas,\r\n                buscarRecetas,\r\n                guardarConsultar\r\n            }}\r\n        >\r\n            {props.children}\r\n        </RecetasContext.Provider>\r\n    );\r\n}\r\n\r\nexport default RecetasProvider;","C:\\laragon\\www\\udemy\\react-bebidas\\src\\context\\CategoriaContext.js",[],"C:\\laragon\\www\\udemy\\react-bebidas\\src\\components\\Formulario.js",[],"C:\\laragon\\www\\udemy\\react-bebidas\\src\\components\\Header.js",[],"C:\\laragon\\www\\udemy\\react-bebidas\\src\\components\\ListaRecetas.js",[],"C:\\laragon\\www\\udemy\\react-bebidas\\src\\components\\Receta.js",["47"],"C:\\laragon\\www\\udemy\\react-bebidas\\src\\context\\ModalContext.js",[],{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","severity":1,"message":"53","line":31,"column":7,"nodeType":"54","endLine":31,"endColumn":17,"suggestions":"55"},{"ruleId":"56","severity":1,"message":"57","line":78,"column":29,"nodeType":"58","endLine":78,"endColumn":94},"no-native-reassign",["59"],"no-negated-in-lhs",["60"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'categoria', 'consultar', and 'nombre'. Either include them or remove the dependency array.","ArrayExpression",["61"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation",{"desc":"62","fix":"63"},"Update the dependencies array to be: [busqueda, categoria, consultar, nombre]",{"range":"64","text":"65"},[904,914],"[busqueda, categoria, consultar, nombre]"]